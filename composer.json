{
  "name": "b-strauss/typo3-demo",
  "description": "Typo3 cms demo project",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Strau√ü",
      "homepage": "https://github.com/b-strauss"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms": "~8.4.0",
    "dmitryd/typo3-realurl": "~2.1.0",
    "fluidtypo3/vhs": "~3.1.0",
    "b-strauss/engine": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "BStrauss\\Engine\\": "public/typo3conf/ext/engine/Classes",
      "BStrauss\\Typo3Demo\\News\\": "public/typo3conf/ext/news/Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public"
    }
  }
}